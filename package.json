{
  "name": "vapor-master",
  "version": "0.1.0",
  "main": "./index.js",
  "directories": {
    "js": "js"
  },
  "scripts": {
    "test": "./node_modules/.bin/eslint ./",
    "start": "node ./dist/vapor-master.js",
    "watch": "DEBUG=roshub.*,-roshub_crypto.*,-roshub.model.* nodemon index.js",
    "build": "webpack --config webpack.dev.js",
    "build:catkin-pkg-melodic": "rm -rf debian obj-*; bloom-generate rosdebian --os-name ubuntu --os-version bionic --ros-distro melodic; fakeroot debian/rules binary",
    "build:catkin-pkg-kinetic": "rm -rf debian obj-*; bloom-generate rosdebian --os-name ubuntu --os-version xenial --ros-distro kinetic; fakeroot debian/rules binary"
  },
  "bin": {
    "vapor-master": "bin/vapor-master"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:roshub/vapor_master.git"
  },
  "author": "michael philetus weller <philetus@roshub.io>",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "bossy": "^4.0.1",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "express-xmlrpc": "NickZ/express-xmlrpc#multicall",
    "mkdirp": "^0.5.1",
    "mongoose": "5.2.7",
    "mongoose-schema-jsonschema": "^1.1.15",
    "morgan": "^1.9.0",
    "nconf": "^0.10.0",
    "nodemon": "^1.18.4",
    "rimraf": "^2.6.2",
    "sanitize-filename": "^1.6.1",
    "url-parse": "^1.4.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@webpack-cli/init": "^0.1.2",
    "babel-loader": "^8.0.4",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-tree-dependency": "^1.1.0"
  }
}
